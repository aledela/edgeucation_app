<p id="notice"><%= notice %></p>

<%= link_to "Back", new_search_path, class: "back-btn" %>

<center>
  <h3>Search Result</h3>
  <table>
    <thead>
      <tr>
        <th>Institutions</th>
        <th>Zip code</th>
        <th>Cost of Attendence</th>
        <th colspan="3"></th>
      </tr>
    </thead>
  
    <tbody>
      <% @search.each do |college| %>
        <tr>
          <td><%= college.coll_name %></td>
          <td><%= college.zip_code %></td>
          <td><%= college.cost_att %></td>
          <td><%= link_to 'Show', college %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</center>
<%= will_paginate @search %>